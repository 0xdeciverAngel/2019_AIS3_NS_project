{
    "name": "OCR Filter",
    "version": "0.0",
    "description": "AIS3 2019 Final Project",
    "manifest_version": 2,
  
    "content_scripts": [{
      "matches": ["https://*.facebook.com/*"],
      "js": ["script/watch.js"]
    }],
  
    "permissions": ["webRequest", "https://*.facebook.com/*", "http://*.facebook.com/*"]
  }